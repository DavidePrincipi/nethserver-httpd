{
    use File::Basename;
    my $path = "/var/lib/nethserver/vhost/shared-folder-migration";
    if ( ! -d $path ) {
        return '# No migrated shared folder!';
    }
    foreach my $dir (glob "$path/*") {
        my $n = basename($dir); 
        $OUT .= "Alias /$n $dir\n";
        $OUT .= "<Directory $dir>\n";
        $OUT .= "  AllowOverride All\n";
        $OUT .= "  Require all granted\n";
        $OUT .= "</Directory>\n";
    }
}
